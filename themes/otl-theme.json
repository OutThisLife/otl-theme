{
  "$schema": "vscode://schemas/color-theme",
  "name": "OTL",
  "type": "dark",
  "colors": {
    "editor.background": "#000080",
    "activityBar.background": "#111",
    "dropdown.background": "#111",
    "editorGroupHeader.tabsBackground": "#111",
    "input.background": "#111",
    "list.activeSelectionBackground": "#000080",
    "list.activeSelectionForeground": "#ccc",
    "list.errorForeground": "#f76e6e",
    "list.hoverBackground": "#000080",
    "list.hoverForeground": "#ccc",
    "list.inactiveSelectionBackground": "#000080",
    "list.inactiveSelectionForeground": "#ccc",
    "list.warningForeground": "#d15dcf",
    "sideBar.background": "#111",
    "sideBar.border": "#111",
    "statusBar.background": "#111",
    "statusBar.border": "#111",
    "tab.activeBackground": "#000080",
    "tab.activeBorder": "#2497E3",
    "tab.border": "#000080",
    "tab.inactiveBackground": "#111",
    "terminal.background": "#000080"
  },
  "settings": {
    "background": "#000080",
    "caret": "#00ff00",
    "foreground": "#00bbbb",
    "invisibles": "#ffffff26",
    "lineHighlight": "#ffffff44",
    "selection": "#ffffff66"
  },
  "tokenColors": [
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation - (punctuation.definition.string || punctuation.definition.comment)",
      "settings": {
        "foreground": "#55ff55"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#55ffff"
      }
    },
    {
      "name": "Entity",
      "scope": "entity",
      "settings": {
        "foreground": "#55ffff"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#ff55ff"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#ff55ff"
      }
    },
    {
      "name": "String",
      "scope": [
        "string -string.unquoted.old-plist -string.unquoted.heredoc",
        "string.unquoted.heredoc string"
      ],
      "settings": {
        "foreground": "#00bb00"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#bbbb00"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "#55ffff"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "source.glsl"],
      "settings": {
        "foreground": "#55ff55"
      }
    },
    {
      "name": "Lang Variable",
      "scope": "variable.language",
      "settings": {
        "foreground": "#55ff55"
      }
    },
    {
      "name": "Function Call",
      "scope": ["meta.function-call", "support.function"],
      "settings": {
        "foreground": "#55ffff"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#F8F8F8"
      }
    },
    {
      "name": "Embedded Source",
      "scope": ["text source", "string.unquoted.heredoc", "source source"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Entity inherited-class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffffff"
      }
    },
    {
      "name": "String embedded-source",
      "scope": "string.quoted source",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "String constant",
      "scope": "string constant",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "String.regexp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#00bb00"
      }
    },
    {
      "name": "String variable",
      "scope": "string variable",
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "name": "Support.function",
      "scope": "support.function",
      "settings": {
        "foreground": "#ffff55"
      }
    },
    {
      "name": "Support.constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Exception",
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "name": "C/C++ Preprocessor Line",
      "scope": "meta.preprocessor.c",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "C/C++ Preprocessor Directive",
      "scope": "meta.preprocessor.c keyword",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Doctype/XML Processing",
      "scope": [
        "meta.sgml.html meta.doctype",
        "meta.sgml.html meta.doctype entity",
        "meta.sgml.html meta.doctype string",
        "meta.xml-processing",
        "meta.xml-processing entity",
        "meta.xml-processing string"
      ],
      "settings": {
        "foreground": "#00ff00"
      }
    },
    {
      "name": "Meta.tag.A",
      "scope": ["meta.tag", "meta.tag entity"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "css tag-name",
      "scope": "meta.selector.css entity.name.tag",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "css#id",
      "scope": "meta.selector.css entity.other.attribute-name.id",
      "settings": {
        "foreground": "#55ffff"
      }
    },
    {
      "name": "css.class",
      "scope": "meta.selector.css entity.other.attribute-name.class",
      "settings": {
        "foreground": "#55ffff"
      }
    },
    {
      "name": "css property-name:",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#bbbbbb"
      }
    },
    {
      "name": "css property-value;",
      "scope": [
        "meta.property-group support.constant.property-value.css",
        "meta.property-value support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "css @at-rule",
      "scope": "meta.preprocessor.at-rule keyword.control.at-rule",
      "settings": {
        "foreground": "#ff55ff"
      }
    },
    {
      "name": "css additional-constants",
      "scope": [
        "meta.property-value support.constant.named-color.css",
        "meta.property-value constant"
      ],
      "settings": {
        "foreground": "#55ff55"
      }
    },
    {
      "name": "css constructor.argument",
      "scope": "meta.constructor.argument.css",
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "name": "diff.header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#F8F8F8"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#F8F8F8"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#F8F8F8"
      }
    },
    {
      "name": "Raw Markup",
      "scope": "markup.raw",
      "settings": {}
    },
    {
      "name": "Block Quote",
      "scope": "markup.quote",
      "settings": {}
    },
    {
      "name": "List",
      "scope": "markup.list",
      "settings": {}
    },
    {
      "name": "Bold Markup",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#55ffff"
      }
    },
    {
      "name": "Italic Markup",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#55ffff"
      }
    },
    {
      "name": "Heading Markup",
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#55ffff"
      }
    },
    {
      "scope": [
        "punctuation.definition.template-expression.end",
        "punctuation.definition.template-expression.begin",
        "comment",
        "constant",
        "emphasis",
        "entity.name.function",
        "entity.name.type.class",
        "entity.other.attribute-name",
        "entity.other.inherited-class",
        "keyword",
        "markup.italic",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded",
        "storage.type.class.js",
        "storage",
        "variable.language"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["invalid", "storage.type.function.arrow"],
      "settings": {
        "fontStyle": ""
      }
    }
  ]
}
